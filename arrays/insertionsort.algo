ALGORITHM insertion_sort
VAR
    i, j, temp : INTEGER;
    cards : ARRAY_OF INTEGER[6] := {4,3,6,7,3,2};
BEGIN
    FOR i FROM 1 TO cards.length - 1 STEP 1 DO
        temp = cards[i]
        j = i - 1;
        WHILE (j >= 0 AND temp <= cards[j]) DO
            cards[j+1] = cards[j];
            j = j - 1;
        END_WHILE
        cards[j+1] = temp
    END_FOR
END